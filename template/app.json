{
  <% const slug = this.slugify(name.toLowerCase()) -%>
  "expo": {
    "name": "<%= name %>",
    "description": "<%= description %>",
    "slug": "<%= slug %>",
    "privacy": "unlisted",
    "sdkVersion": "22.0.0",
    "version": "1.0.0",
    "orientation": "portrait",
    "primaryColor": "#fff",
    "icon": "./assets/icons/app-icon.png",
    "loading": {
      "icon": "./assets/icons/loading-icon.png"
    },
    "packagerOpts": {
      "assetExts": ["ttf"]
    },
    "ios": {
      "supportsTablet": true
    }
  }
}
